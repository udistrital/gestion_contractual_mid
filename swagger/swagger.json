    "/espacios-fisicos/sedes": {
      "get": {
        "description": "Obtiene todas las sedes activas del sistema",
        "operationId": "EspaciosFisicosController_obtenerSedes",
        "parameters": [],
        "responses": {
          "200": {
            "description": "Lista de sedes obtenida exitosamente",
            "content": {
              "application/json": {
                "schema": {
                  "allOf": [
                    {
                      "properties": {
                        "Success": {
                          "type": "boolean",
                          "example": true
                        },
                        "Status": {
                          "type": "number",
                          "example": 200
                        },
                        "Message": {
                          "type": "string",
                          "example": "Sedes obtenidas exitosamente"
                        },
                        "Data": {
                          "type": "array",
                          "items": {
                            "properties": {
                              "Id": {
                                "type": "number",
                                "example": 1
                              },
                              "Nombre": {
                                "type": "string",
                                "example": "Sede Principal"
                              }
                            }
                          }
                        }
                      }
                    }
                  ]
                }
              }
            }
          },
          "404": {
            "description": "",
            "content": {
              "application/json": {
                "schema": {
                  "properties": {
                    "Success": {
                      "type": "boolean",
                      "example": false
                    },
                    "Status": {
                      "type": "number",
                      "example": 404
                    },
                    "Message": {
                      "type": "string",
                      "example": "No se encontraron sedes activas"
                    }
                  }
                }
              }
            }
          }
        },
        "summary": "Obtener sedes",
        "tags": [
          "Espacios Físicos"
        ]
      }
    },
